{
  "name": "reporting-engine",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "node --max-old-space-size=4096 node_modules/eslint/bin/eslint.js -c .eslintrc.js .",
    "lint:fix": "eslint -c .eslintrc.js . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^11.1.13",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/websockets": "^11.1.13",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bullmq": "^5.67.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.4",
    "joi": "^18.0.2",
    "openai": "^6.18.0",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.8.3",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.2.2",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^48.11.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "prisma": "^7.3.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "^5.9.3",
    "webpack": "^5.105.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "test/.*\\.spec\\.ts$",
    "moduleNameMapper": {
      "^@providers$": "<rootDir>/src/providers",
      "^@providers/(.*)$": "<rootDir>/src/providers/$1",
      "^@common$": "<rootDir>/src/common",
      "^@common/(.*)$": "<rootDir>/src/common/$1",
      "^@reportsModels$": "<rootDir>/src/models/reports",
      "^@reportsModels/(.*)$": "<rootDir>/src/models/reports/$1",
      "^@src/(.*)$": "<rootDir>/src/$1",
      "^@config/(.*)$": "<rootDir>/config/$1",


      "^@reportsControllers/(.*)$": ["<rootDir>/src/models/reports/controllers/$1"],
      "^@reportsControllers$": ["<rootDir>/src/models/reports/controllers"],

      "^@reportsEnums/(.*)$": ["<rootDir>/src/models/reports/enum/$1"],
      "^@reportsEnums$": ["<rootDir>/src/models/reports/enum"],

      "^@reportsProcessors/(.*)$": ["<rootDir>/src/models/reports/processors/$1"],
      "^@reportsProcessors$": ["<rootDir>/src/models/reports/processors"],

      "^@reportsUtils/(.*)$": ["<rootDir>/src/models/reports/utils/$1"],
      "^@reportsUtils$": ["<rootDir>/src/models/reports/utils"],

      "^@reportsValidationSchema/(.*)$": [
        "<rootDir>/src/models/reports/validation-schema/$1"
      ],
      "^@reportsValidationSchema$": ["<rootDir>/src/models/reports/validation-schema"],

      "^@reportsNotification/(.*)$": ["<rootDir>/src/models/reports/notification/$1"],
      "^@reportsNotification$": ["<rootDir>/src/models/reports/notification"],

      "^@reportsServices/(.*)$": ["<rootDir>/src/models/reports/services/$1"],
      "^@reportsServices$": ["<rootDir>/src/models/reports/services"]
    },
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "config/**/*.ts",
      "prisma/**/*.ts",
      "!src/**/*.spec.ts",
      "!src/models/reports/processors/**",
      "!src/models/reports/controllers/reports.controller.ts",
      "!src/models/reports/notification/reports.gateway.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/coverage/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  }
}
